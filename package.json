{
  "name": "stikom-backend",
  "module": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts  --outdir ./build --target bun",
    "start": "NODE_ENV=production bun run build/index.js",
    "build-tsc": "tsc -p index.ts --outdir dist",
    "apply-migration": "bun drizzle-kit push:pg",
    "generate-migration": "bun drizzle-kit generate:pg",
    "apply-seeders": "bun src/db/seeders/index.ts",
    "revert-seeders": "bun src/db/seeders/clear-table.ts"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/register": "^7.23.7",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/swagger": "^1.2.2",
    "@types/node": "^20.11.17",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.41.0",
    "elysia": "^1.2.25",
    "elysia-rate-limit": "^4.2.1",
    "lodash": "^4.17.21",
    "oslo": "^1.1.0",
    "pg": "^8.11.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "uuidv7": "^0.6.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/lodash": "^4.17.16",
    "@types/pg": "^8.11.0",
    "drizzle-kit": "^0.20.14",
    "tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
